                    <CheckBox
                        android:id="@+id/chkWhenGoodTimesGalleryReady"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginRight="13dp"
                        android:button="@drawable/btn_check_notifications" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayoutten"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="1dp"
                    android:background="@drawable/transparency_all_artist"
                    android:gravity="center_horizontal">

                    <uk.co.wehive.hive.utils.CustomFontTextView
                        android:id="@+id/ten"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/chkWhenEventAboutToStart"
                        android:gravity="center_vertical"
                        android:text="@string/trackedevent"
