<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/general_background">

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/relativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/transparency_all_artist"
                    android:gravity="center_horizontal">

                    <uk.co.wehive.hive.utils.CustomFontTextView
                        android:id="@+id/one"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="20dp"
                        android:layout_toLeftOf="@+id/chkWhenFollowMe"
                        android:gravity="center_vertical"
                        android:text="@string/someone"
                        android:textColor="@android:color/white"
                        android:textStyle="normal"
                        app:fontName="Raleway-Light.ttf" />

                    <CheckBox
                        android:id="@+id/chkWhenFollowMe"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginRight="13dp"
                        android:button="@drawable/btn_check_notifications" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayouttwo"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="1dp"
                    android:background="@drawable/transparency_all_artist"
                    android:gravity="center_horizontal">

                    <uk.co.wehive.hive.utils.CustomFontTextView
                        android:id="@+id/two"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/chkWhenCommentMyPost"
                        android:gravity="center_vertical"
                        android:text="@string/posted"
                        android:textColor="@android:color/white"
                        android:textStyle="normal"
                        app:fontName="Raleway-Light.ttf" />

                    <CheckBox
                        android:id="@+id/chkWhenCommentMyPost"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginRight="13dp"
                        android:button="@drawable/btn_check_notifications" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayouttree"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
