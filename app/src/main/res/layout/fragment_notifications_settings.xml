                        android:textStyle="normal"
                        app:fontName="Raleway-Light.ttf" />

                    <CheckBox
                        android:id="@+id/chkWhenTrackedArtistPost"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginRight="13dp"
                        android:button="@drawable/btn_check_notifications" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayoutnine"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="1dp"
                    android:background="@drawable/transparency_all_artist"
                    android:gravity="center_horizontal">

                    <uk.co.wehive.hive.utils.CustomFontTextView
                        android:id="@+id/nine"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
